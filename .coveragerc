
[run]
branch = True
source = stoq/
omit =
    */stoq/tests/*
    */stoq/cli.py
    */stoq/installer.py
    */stoq/utils.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if raven_imported and self.sentry_url:
    pass
